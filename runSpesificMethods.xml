<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="run method">
    <!-- to run spesific methods, use <include>, otherwise use <exclude>-->
    <test name="run spesific methods">

        <classes>

            <class name="tests.D02_SoftAssert.C04_negativeLoginTest">

                <methods>
                    <include name="wrongEmailTest"></include>
                </methods>

            </class>

            <class name="tests.D03_SmokeTests.C02_negativeLoginTest">
                <methods>
                    <exclude name="wrongPasswordTest"></exclude>
                </methods>
            </class>

        </classes>

    </test>
</suite>